# include <stdio.h>
# include <stdlib.h>

int temperature;

main () {
    system("cls");
    printf("Saisir la tempÃ©rature\n");
	scanf("%d", &temperature);

    if (temperature <= 0) {
        printf("C'est de la glace\n");
    } else {
        if (temperature < 100) {
            printf("Eau");
        } else {
            printf("Vapeur");
        }
    }
    system("pause");
}